import org.springframework.web.bind.annotation.RestController;

@RestController
@RequestMapping("/hello")
public class HelloController {

	@GetMapping
	public String getHello () {
		return "Hello turma 44!";
	}
	
	@GetMapping("/turma 44")
	public String getHello2 () {
		return "A turma 44 Ã© top!";
	}
}
